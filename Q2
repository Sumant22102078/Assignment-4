year = int(input("Enter an year- "))
if (year % 4 == 0 and not year % 100 == 0) or year % 400 == 0:
    if year >= 2024:
        print(f"{year} is a Leap Year.")
    else:
        print(f"{year} was a Leap Year.")
else:
    if year >= 2024:
        print(f"{year} is not a Leap Year.")
    else:
        print(f"{year} was not a Leap Year.")
